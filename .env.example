# Copy this file to .env and customize values
# These variables are used by docker-compose to configure the app and database

# Postgres settings
POSTGRES_DB=marks-manager-db
POSTGRES_USER=umm_user
POSTGRES_PASSWORD=change_me

# App settings
# Toggle extra debug endpoints (use "true" or "false")
ENABLE_DEBUG_ROUTES=false

# Secret key for server-side sessions (required)
# Generate a strong random string (length >= 32). Example (PowerShell):
#   [guid]::NewGuid().ToString() + [guid]::NewGuid().ToString()
# Or Python:
#   python -c "import secrets; print(secrets.token_urlsafe(32))"
SESSION_SECRET_KEY=change_me_replace_with_secure_random
